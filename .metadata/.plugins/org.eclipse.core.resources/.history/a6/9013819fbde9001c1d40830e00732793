package stepDefinitions;

import cucumber.api.PendingException;
import cucumber.api.java.en.Given;
import cucumber.api.java.en.When;
import cucumber.api.java.en.Then;
import cucumber.api.java.en.And;
import cucumber.api.junit.Cucumber;
import org.junit.runner.RunWith;

@RunWith(Cucumber.class)
public class stepDefinitions {

	
	



package my.package.name






    @Given("^User is on Landing page$")
    public void user_is_on_landing_page() throws Throwable {
	System.out.println("User is on Landing page");
        throw new PendingException();
    }

    @When("^User Logins with id and password$")
    public void user_logins_with_id_and_password() throws Throwable {
    	System.out.println("User is on Landing page");
        throw new PendingException();
    }

    @Then("^Homepage is displayed$")
    public void homepage_is_displayed() throws Throwable {
    	System.out.println("User is on Landing page");
        throw new PendingException();
    }

    @And("^click sigin button$")
    public void click_sigin_button() throws Throwable {
    	System.out.println("User is on Landing page");
        throw new PendingException();
    }

    @And("^All cards are displaying or not$")
    public void all_cards_are_displaying_or_not() throws Throwable {
    	System.out.println("User is on Landing page");
        throw new PendingException();
    }

}
